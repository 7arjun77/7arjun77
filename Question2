class Student:
		 def __init__(self, name, grade, age):
			    self.name = name
			    self.grade = grade
			    self.age = age
		def display():
			print("Name :",name)
			print ("Grade:", grade)
			print("Age:",age)
	class School (Student):
		def __init__(self, name, grade, age):
			super().__init__(self, name, grade, age)
		def SchoolStudentDisplay():
			print("Name :",name)
			print ("Grade:", grade)
			print("Age:",age)
